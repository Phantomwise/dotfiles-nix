	╚══════════════════════════════════════════════════════════════╝
	*/

	"temperature": {
        // "thermal-zone": 6,
        // "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
        "critical-threshold": 70,
        // "format-critical": "{temperatureC}°C {icon}",
        "format": "<span size='large' rise='-1500'>{icon}</span>  {temperatureC}°C",
