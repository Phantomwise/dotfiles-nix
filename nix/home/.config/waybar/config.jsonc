{
	"layer": "top",
	"position": "bottom",
	"margin": 0,
	"height": 25,

/*
███████████████████████████████████████████████████████████████╗
█╔════════════════════════════════════════════════════════════█║
█║░░░░░░░░░░░░░░░░░░░░░░ MODULES LAYOUT ░░░░░░░░░░░░░░░░░░░░░░█║
███████████████████████████████████████████████████████████████║
╚══════════════════════════════════════════════════════════════╝
*/

	"modules-left": [
		"custom/arch",
		"custom/right-arrow-light-dark",
		"sway/workspaces",
		"custom/right-arrow-dark-light",
		"sway/window#shell",
		"custom/right-arrow-light-dark",
		"sway/mode",
		"sway/window#title",
		"custom/right-arrow-dark-transparent"
	],
	"modules-center": [
		"custom/left-arrow-transparent-dark",
		"clock#day",
		"custom/left-arrow-dark-light",
		"clock#hour",
		"custom/right-arrow-light-dark",
		"clock#date",
		"custom/right-arrow-dark-transparent"
	],
	"modules-right": [
		/*
		"custom/right-arrow-battery",
		*/
		"custom/left-arrow-transparent-dark",
		"battery",
		"custom/left-arrow-dark-light",
		/*
		"custom/left-arrow-battery",
		*/
		"temperature",
		"custom/left-arrow-light-dark",
		"cpu",
		"custom/left-arrow-dark-light",
		"memory",
		"custom/left-arrow-light-dark",
		"disk",
		"custom/left-arrow-dark-light",
		"network",
		"custom/left-arrow-light-dark",
		"pulseaudio",
		"custom/left-arrow-dark-light",
		"sway/language",
		"custom/left-arrow-light-dark",
		"tray"
	],

/*
███████████████████████████████████████████████████████████████╗
█╔════════════════════════════════════════════════════════════█║
█║░░░░░░░░░░░░░░░░░░░░░░░░░ MODULES ░░░░░░░░░░░░░░░░░░░░░░░░░░█║
███████████████████████████████████████████████████████████████║
╚══════════════════════════════════════════════════════════════╝
*/

	/*
	███████████████████████████████████████████████████████████████╗
	█╔════════════════════════════════════════════════════════════█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░░ CLOCK ░░░░░░░░░░░░░░░░░░░░░░░░░░░█║
	███████████████████████████████████████████████████████████████║
	╚══════════════════════════════════════════════════════════════╝
	*/

	/*
	"clock": {
		"format": "{:%H:%M %d/%m/%Y}",
		"tooltip": false
	},
	*/

	"clock#day": {
		"format": "{:%a}",
		"format-alt": "{:%A}",
		"tooltip": true,
		"tooltip-format": "{:%Hh%M %Z\n%A the %d of %B %Y}"
	},

	"clock#hour": {
		/*
		"format": "{:%Hh%M}",
		*/
		"format": "{:%Hh%M}",
		"format-alt": "{:%Hh%M %Z}",
		"timezone": "Europe/Paris",
		"tooltip": true,
		"tooltip-format": "{tz_list}",
		"timezones": [
			"Europe/Paris",
			"Etc/UTC",
			"America/New_York",
			"America/Los_Angeles",
			"Asia/Tokyo"
		]
		/*
		"actions":  {
			"on-click-right": "mode",
			"on-scroll-up": "tz_up",
			"on-scroll-down": "tz_down"
			}
		*/
		},

	"clock#date": {
		"format": "{:%d/%m}",
		"format-alt": "{:%d/%m/%Y}",
		"tooltip": true,
		"tooltip-format": "<tt>{calendar}</tt>",
		"calendar": {
					"mode"          : "year",
					"mode-mon-col"  : 3,
					"weeks-pos"     : "right",
					"on-scroll"     : 1,
					"format": {
							  "months":     "<span color='#ffffff'><b>{}</b></span>",
							  "days":       "<span color='#bbbbbb'><b>{}</b></span>",
							  "weeks":      "<span color='#004BAA'><b>W{}</b></span>",
							  "weekdays":   "<span color='#004BAA'><b>{}</b></span>",
							  "today":      "<span color='#a10000'><b><u>{}</u></b></span>"
							  }
					},
		"actions":  {
					"on-click-right": "mode",
					"on-scroll-up": "shift_down",
					"on-scroll-down": "shift_up"
					}
	},

	/*
	███████████████████████████████████████████████████████████████╗
	█╔════════════════════════════════════════════════════════════█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░░░ SWAY ░░░░░░░░░░░░░░░░░░░░░░░░░░░█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░░ MODES ░░░░░░░░░░░░░░░░░░░░░░░░░░░█║
	███████████████████████████████████████████████████████████████║
	╚══════════════════════════════════════════════════════════════╝
	*/

	"sway/mode": {
		"format": " {}",
		"max-length": 50
	},

	/*
	███████████████████████████████████████████████████████████████╗
	█╔════════════════════════════════════════════════════════════█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░░░ SWAY ░░░░░░░░░░░░░░░░░░░░░░░░░░░█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░ WORKSPACES ░░░░░░░░░░░░░░░░░░░░░░░░█║
	███████████████████████████████████████████████████████████████║
	╚══════════════════════════════════════════════════════════════╝
	*/

	"sway/workspaces": {
		"disable-scroll": true,
		"all-outputs": true,
		"format": "{name}",
		"on-scroll-up": "swaymsg workspace prev",
		"on-scroll-down": "swaymsg workspace next"
		/*
		"format": "{name}: {icon}"
		"format-icons": {
			"1": "",
			"2": "",
			"3": "",
			"4": "",
			"5": "",
			"urgent": "",
			"focused": "",
			"default": "",
			"high-priority-named": ["1", "2"]
		}
		*/
	},

	/*
	███████████████████████████████████████████████████████████████╗
	█╔════════════════════════════════════════════════════════════█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░░░ SWAY ░░░░░░░░░░░░░░░░░░░░░░░░░░░█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░░ WINDOW ░░░░░░░░░░░░░░░░░░░░░░░░░░█║
	███████████████████████████████████████████████████████████████║
	╚══════════════════════════════════════════════════════════════╝
	*/

	"sway/window#shell": {
		"format": "{shell}",
		"max-length": 50,
		/*
		"rewrite": {
			"xdg_shell": "XDG Shell",
			"xwayland": "XWayland",
			// Pointless, adding them only to remember they exist :
			"fullscreen_shell": "Fullscreen Shell",
			"layer_shell": "Layer Shell"
		},
		*/
		"on-scroll-up": "swaymsg workspace prev",
		"on-scroll-down": "swaymsg workspace next",
		"tooltip":false
	},

	/*
	"sway/window#class": {
		"format": "{class}",
		"max-length": 50,
		"on-scroll-up": "swaymsg workspace prev",
		"on-scroll-down": "swaymsg workspace next"
	},
	*/
	/* Not available */

	"sway/window#title": {
		"format": "{title}",
		"max-length": 50,
		"on-click-middle": "swaymsg kill",
		"on-scroll-up": "swaymsg workspace prev",
		"on-scroll-down": "swaymsg workspace next",
		"tooltip":false,
		"rewrite": {
			/*
			"(.*) - VSCodium": "  $1",
			"(.*) - Mozilla Firefox": "󰈹  $1"
			"(.*) - Mozilla Firefox": "🌎 $1",
			"(.*) - vim": " $1",
			"(.*) - zsh": " [$1]"
			*/
		}
	},

	/*
	███████████████████████████████████████████████████████████████╗
	█╔════════════════════════════════════════════════════════════█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░ HYPRLAND ░░░░░░░░░░░░░░░░░░░░░░░░░█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░░ WINDOW ░░░░░░░░░░░░░░░░░░░░░░░░░░█║
	███████████████████████████████████████████████████████████████║
	╚══════════════════════════════════════════════════════════════╝
	*/

	/*
	"hyprland/window#title": {
		"format": "{title}",
		"max-length" : 30,
		"on-click-middle": "hyprctl dispatch killactive",
		"on-scroll-up": "hyprctl dispatch workspace e-1",
		"on-scroll-down": "hyprctl dispatch workspace e+1"
	},
	*/

	/*
	███████████████████████████████████████████████████████████████╗
	█╔════════════════════════════════════════════════════════════█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░░░ SWAY ░░░░░░░░░░░░░░░░░░░░░░░░░░░█║
	█║░░░░░░░░░░░░░░░░░░░░░░░░░ LANGAGUE ░░░░░░░░░░░░░░░░░░░░░░░░░█║
